<div class="container">
  <%= form_for @tweet do |f| %>
  
  <h1>編集する</h1>
  
 <div class="field">
   <p>コメント</p>
   <%= f.text_field :description %>
 </div></br>
 
 <div class="field">
   <%= f.file_field :picture %></br>
 </div>
 
 <%= f.submit "投稿" %>
 <% end %></br>
 <%= button_to "削除", tweet_path(@tweet.id), method: :delete, data: { confirm: '削除しますか？' } %>
 
 </div>
 
 